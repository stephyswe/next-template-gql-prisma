{
    "name": "nextjs-ts-auto-graphql",
    "version": "1.0.0",
    "private": false,
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@emotion/cache": "^11.10.3",
        "@emotion/react": "^11.10.4",
        "@emotion/server": "^11.10.0",
        "@emotion/styled": "^11.10.4",
        "@mui/material": "^5.10.3",
        "@prisma/client": "4.3.1",
        "@types/graphql-fields": "^1.3.4",
        "apollo-server-core": "^3.10.2",
        "apollo-server-micro": "^3.10.2",
        "graphql": "^15.8.0",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.18.0",
        "jest-environment-jsdom": "^29.0.2",
        "jest-esm-transformer": "^1.0.0",
        "micro": "^9.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.34.2",
        "reflect-metadata": "^0.1.13",
        "ts-jest": "^28.0.8",
        "type-graphql": "^1.1.1"
    },
    "keywords": [
        "Next.js",
        "GraphQL",
        "TypeScript",
        "Prisma",
        "Apollo",
        "ESLint",
        "Prettier",
        "React",
        "React Hook Form",
        "Jest",
        "Testing Library React",
        "Material-UI"
    ],
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "scripts": {
        "build": "next build",
        "deploy": "prisma migrate deploy",
        "dev": "prisma generate && concurrently \"npm run server-dev\" \"npm run watch-queries\" \"npm run type-check:watch\"",
        "format": "prisma format &&  prettier --write . && eslint --fix .",
        "generate": "prisma generate",
        "generate-types": "graphql-codegen --config codegen.yml",
        "lint": "next lint",
        "migrate": "prisma migrate dev",
        "postinstall": "prisma generate",
        "prepare": "husky install",
        "prisma": "prisma format && npm run migrate && npm run seed",
        "quality": "npm install --silent && npm run type-check && npm run lint && npm run test",
        "reset": "prisma migrate reset",
        "seed": "prisma db seed",
        "server-dev": "next",
        "start": "next start",
        "studio": "prisma studio",
        "test": "jest",
        "type-check": "tsc",
        "type-check:watch": "tsc --watch --preserveWatchOutput",
        "update-deps": "npx npm-check-updates -i",
        "watch-queries": "graphql-codegen --config codegen.yml --watch"
    },
    "devDependencies": {
        "@axe-core/react": "^4.4.4",
        "@babel/core": "^7.18.13",
        "@babel/node": "^7.18.10",
        "@babel/plugin-transform-react-jsx": "^7.18.10",
        "@babel/plugin-transform-typescript": "^7.18.12",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@graphql-codegen/cli": "^2.12.0",
        "@graphql-codegen/fragment-matcher": "^3.3.1",
        "@graphql-codegen/typescript": "^2.7.3",
        "@graphql-codegen/typescript-operations": "^2.5.3",
        "@graphql-codegen/typescript-react-apollo": "^3.3.7",
        "@next/eslint-plugin-next": "^12.2.5",
        "@testing-library/dom": "^8.17.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/node": "^18.7.14",
        "@types/react": "^18.0.18",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^28.1.3",
        "concurrently": "^7.3.0",
        "eslint": "^8.23.0",
        "eslint-config-next": "^12.2.5",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.6",
        "husky": "^8.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^28.1.3",
        "next": "^12.2.5",
        "prettier": "^2.7.1",
        "prisma": "4.3.1",
        "prisma-erd-generator": "^1.1.0",
        "ts-node": "^10.9.1",
        "typegraphql-prisma": "^0.21.4",
        "typescript": "^4.8.2"
    }
}
